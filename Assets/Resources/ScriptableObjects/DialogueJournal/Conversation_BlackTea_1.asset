  - characterName_CN: "\u7EA6\u7FF0\u900A"
    characterName_EN: John
    dialogue_CN: "\u6211\u60F3\u4ECA\u5929\u6211\u4EEC\u5E94\u8BE5\u8BA8\u8BBA\u8FD9\u6279\u8BA2\u5355\u7684\u4EA4\u8D27\u65F6\u95F4\u95EE\u9898\u3002"
    dialogue_EN: 'I think today we should discuss the delivery time issue for this
      batch of orders. '
    color: {r: 0, g: 0, b: 0, a: 1}
  - characterName_CN: "\u674E\u6587\u4FCA"
    characterName_EN: LiWenJun
